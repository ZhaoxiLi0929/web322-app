<div class="row">
 <div class="col-md-12">
 <h2>Cars
     <a href="/cars/add" class="btn btn-success pull-right">Add New Car</a> 
 </h2>
 <hr />
 <div class = "table-responsive">
    <table class = "table">
        <thead><tr><th>Car Make</th><th>Car Model</th><th>Car Year</th><th>Car VIN</th></tr></thead>
    <tbody>
    {{#each cars}}
    <tr><td><a href=" /stores?retailer={{make}}">{{make}}</td><td>{{model}}</td><td><a href="/cars?year={{year}}">{{year}}</a></td><td><a href="/car/{{vin}}">{{vin}}</a></td><td><a class="btn btn-danger" href="cars/delete/{{vin}}">Remove</a></td></tr>
    {{else}}
    <tr><td>No cars found</td></tr>
    {{/each}}
    </tbody>
    </table>
</div>
 </div>
</div> 